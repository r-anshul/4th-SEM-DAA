#include <stdio.h>  //w1q1(c)

int main()
{
   int n,c=0,k;
   printf("Enter size - ");
   scanf("%d",&n);
   int A[n];
   printf("Enter elements - ");
   for(int i=0;i<n;i++)
   {
       scanf("%d",&A[i]);
   }
   printf("Enter element to be searched - ");
   scanf("%d",&k);
   for(int i=0;i<n;i++)
   {
       c++;
       if(k==A[i])
       {
       printf("Element Found\n");
       break;
       }
   }
   printf("Number of comparisons - %d",c);
   return 0;
}
