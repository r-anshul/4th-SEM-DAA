#include <iostream> //w2q3
using namespace std;

int main()
{
    int f = 0;
    int t,k;
    cout<<"Enter Test Cases - ";
    cin>>t;
    for (int j=1;j<=t;j++)
    {
        int n,c=0,k,l=0,m,h;
        cout<<"\nEnter size - ";
        cin>>n;
        int A[n];
        cout<<"Enter elements - ";
        for(int i=0;i<n;i++)
            cin>>A[i];
            cout<<"Enter Key - ";
            cin>>k;
        cout<<"Pairs  - "<<endl;
        for(int i=0;i<n;i++)
        {
            for(int j=i+1;j<n;j++)
            {
                if((A[i]-A[j])==k||(A[j]-A[i])==k)
                f++;
            }
        }
        cout<<f;
    }
    return 0;
}
